            <kind>1</kind>
            <string>/// Player input
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(ord('V'));
key_down = keyboard_check(vk_down);
key_up = keyboard_check(vk_up);

//Realizar inputs
move = key_left + key_right; 
hsp = move * movespeed;  

if move = 0
{
sprite_index=sprPlumas;
}

if move =-1
{
    sprite_index=sprPlumasCaminando; 
    image_xscale=(-1)
}
if move =1
{   
    sprite_index=sprPlumasCaminando; 
    image_xscale=(1)
}

if (vsp&lt;10) vsp += grav;

if(place_meeting(x,y+1,objColisionFather)) 
{
    jumps=jumpsmax;
}
if(key_jump) &amp;&amp; (jumps&gt;0)
{
    jumps-=1;
    vsp=-jumpspeed;
}

if (key_jump) &amp;&amp; (place_meeting(x+1,y,objColisionFather)  || place_meeting(x-1,y,objColisionFather)) 
{
    vsp = -jumpspeed;
}

var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

//ladder
if(key_up || key_down)
{
    if place_meeting(x,y,objLadder) ladder = true;
}
if (ladder)
{
    vsp = 0;
    
    if (key_up) vsp = -2; sprite_index=sprPlumasTrepando; 
    if (key_down) vsp = 2; sprite_index=sprPlumasTrepando; 
    if (key_right) hsp = 2; sprite_index=sprPlumasTrepando;
    if (key_left) hsp = -2; sprite_index=sprPlumasTrepando;
    if !place_meeting(x,y,objLadder) ladder = false;
    if (key_jump) {ladder = false; vsp=-8}
    sprite_index=sprPlumasTrepando;
}


//Colision Horizontal

if (place_meeting(x+hsp_final,y,objColisionFather))
        {
        while(!place_meeting(x+sign(hsp_final),y,objColisionFather)) x += sign(hsp_final);
        hsp_final = 0;
        hsp = 0;
        }
x += hsp_final;




var vsp_final = vsp + vsp_carry;
vsp_carry = 0;

//Vertical Collision
if (place_meeting(x,y+vsp_final,objColisionFather))
{
    while(!place_meeting(x,y+sign(vsp_final),objColisionFather))
    {
        y += sign(vsp_final);
    }
    vsp_final = 0;
    vsp = 0;
}
y += vsp_final;

if(ladder=false)
{
if(vsp_final&gt;0 || vsp_final&lt;0)
{
sprite_index=sprPlumasVuelo;
}
}


</string>
